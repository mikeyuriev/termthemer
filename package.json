{
  "name": "termthemer",
  "version": "0.1.0",
  "description": "Terminal Themer",
  "license": "BSD-3-Clause",
  "dependencies": {
    "parcel-bundler": "^1.12.5"
  },
  "devDependencies": {
    "elm-hot": "^1.1.6",
    "elm-test": "^0.19.1-revision7",
    "node-elm-compiler": "^5.0.6",
    "sass": "^1.35.2"
